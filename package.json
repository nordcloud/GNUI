{
  "name": "@nordcloud/gnui",
  "description": "Nordcloud Design System - a collection of reusable React components used in Nordcloud's SaaS products",
  "version": "2.3.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nordcloud/GNUI.git"
  },
  "author": "Nordcloud Engineering",
  "bugs": {
    "url": "https://github.com/nordcloud/GNUI"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "check:types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx ."
  },
  "engines": {
    "node": ">=14.15.5",
    "npm": ">=7.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check:types"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@storybook/addon-info": "5.3.19",
    "@storybook/addon-knobs": "6.0.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.51",
    "@types/react-date-range": "^0.94.4",
    "@types/react-dom": "^16.9.0",
    "@types/react-modal": "^3.10.5",
    "@types/react-select": "^4.0.13",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.7",
    "babel-loader": "^8.1.0",
    "chroma-js": "^2.1.0",
    "date-fns": "^2.14.0",
    "mini-svg-data-uri": "^1.1.3",
    "polished": "^3.4.4",
    "react": "^16.13.1",
    "react-awesome-styled-grid": "^3.0.12",
    "react-copy-to-clipboard": "^5.0.2",
    "react-date-range": "^1.0.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.11.2",
    "react-scripts": "^3.4.3",
    "react-select": "^4.3.0",
    "reset": "^0.1.0",
    "storybook": "6.0.5",
    "styled": "^1.0.0",
    "styled-components": "^5.0.1",
    "styled-components-breakpoint": "^3.0.0-preview.20",
    "styled-system": "^5.1.5",
    "title": "^3.4.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.5",
    "@storybook/addon-docs": "^6.0.5",
    "@storybook/addon-links": "^6.0.5",
    "@storybook/addons": "^6.0.5",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.5",
    "@types/react-helmet": "^6.1.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "prettier": "2.0.2"
  }
}
